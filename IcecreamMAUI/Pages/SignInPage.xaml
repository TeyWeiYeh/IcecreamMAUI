        <Label 
            Text="Welcome to .NET MAUI! This is the sign in page"
            VerticalOptions="Center" 
            HorizontalOptions="Center" />
        <Button Text="Go To App" Clicked="Button_Clicked"></Button>
    </VerticalStackLayout>
</ContentPage>